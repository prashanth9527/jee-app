{
  "name": "tmp-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=1024 nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:seed": "ts-node prisma/seed.ts",
    "db:seed:math-2025-s1": "ts-node \"prisma/seeds/JEE/Previous Papers/2025/Session1/Maths/seed-runner.ts\"",
    "db:seed:2201-evening": "ts-node \"prisma/seeds/JEE/Previous Papers/2025/Session1/Maths/2201-Mathematics Paper+With+Sol. Evening.enhanced.seed.ts\"",
    "process:pdf-images": "node scripts/process-pdf-images.js",
    "generate:seed-templates": "node scripts/generate-seed-templates.js",
    "db:reset": "npx prisma db push --force-reset && npm run db:seed"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.879.0",
    "@aws-sdk/s3-request-presigner": "^3.879.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "^10.4.20",
    "@nestjs/schedule": "^6.0.0",
    "@prisma/client": "^6.14.0",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "csv-writer": "1.6.0",
    "fast-csv": "5.0.5",
    "fs-extra": "^11.2.0",
    "multer": "^2.0.2",
    "nodemailer": "6.10.1",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "pdf-parse": "^1.1.1",
    "pdf2pic": "^3.2.0",
    "pg-sdk-node": "https://phonepe.mycloudrepo.io/public/repositories/phonepe-pg-sdk-node/releases/v2/phonepe-pg-sdk-node.tgz",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.0",
    "stripe": "14.25.0",
    "twilio": "^4.23.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "6.4.19",
    "@types/passport-jwt": "3.0.13",
    "@types/pdf-parse": "^1.1.5",
    "@types/sharp": "^0.32.0",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "cross-env": "^10.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^6.14.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
